@model List<KukaAgylus.Models.Log>

<div class="row">
    <div class="col-lg-12">
        <h2>Logs</h2>
        <div id="div-log" class="console">
            @if (Model.Count > 0)
            {

                foreach (var log in Model.OrderByDescending(m => m.Time))
                {
                    @log.ToString();<br />
                }

            }else
            {
                
                <span style="font-style:italic">No Logs recorded</span>
            }
        </div>
    </div>
</div>